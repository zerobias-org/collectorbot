{
  "name": "@zerobias-org/collectorbot-avigilon-alta-access",
  "version": "1.0.1",
  "description": "Auditmation Avigilon Alta Access Collectorbot",
  "author": "ctamas@zerobias.com",
  "main": "dist/src/index.js",
  "license": "UNLICENSED",
  "directories": {
    "src": "src"
  },
  "files": [
    "dist",
    "pnpm-lock.yaml",
    "*.yml",
    "*.md"
  ],
  "scripts": {
    "build": "npm run generate && npm run transpile",
    "clean": "rm -rf generated && rm -rf dist",
    "generate:hub-client": "node node_modules/@auditmation/hub-client-codegen/dist/index.js",
    "generate:models": "hub-generator generate -g hub-module -i ./parameters.yml -o generated/ --global-property models,supportingFiles=index.ts && rm ./generated/api/index.ts",
    "generate": "npm run generate:models && npm run generate:hub-client",
    "lint:fix": "eslint --fix src/",
    "lint": "eslint src/",
    "nx:publish": "../../../../scripts/publish.sh",
    "test:integration:watch": "npm run test:integration -- --watch --reporter=min",
    "test:integration": "mocha --exit --inline-diffs --reporter=list test/integration/**/*.ts",
    "test:watch": "npm run test -- --watch --reporter=min",
    "test": "",
    "transpile:watch": "tsc --watch",
    "transpile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:zerobias-org/collectorbot.git",
    "directory": "package/avigilon/alta/access"
  },
  "dependencies": {
    "@auditlogic/schema-auditmation-auditmation-base": "^1.25.1",
    "@auditlogic/schema-auditmation-auditmation-base-ts": "^1.25.1",
    "@auditmation/hub-client": "^8.8.65",
    "@auditmation/hub-core": "^4.5.16",
    "@auditmation/module-auditmation-auditmation-hub": "^3.13.26",
    "@auditmation/module-auditmation-auditmation-platform": "^5.0.33",
    "@auditmation/types-core-js": "^4.9.13",
    "@auditmation/util-collector-utils": "^4.3.6",
    "@zerobias-org/module-avigilon-alta-access": "^0.1.8",
    "@zerobias-org/product-avigilon-alta-access": "^1.0.1",
    "inversify": "^6.0.2",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@auditmation/eslint-config": "^1.1.17",
    "@auditmation/hub-client-codegen": "^2.0.33",
    "@auditmation/hub-secrets-manager": "^2.0.49",
    "@auditmation/util-codegen": "^5.5.22",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^16.3.0",
    "chai": "^4.3.5",
    "mocha": "^9.2.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5"
  },
  "auditmation": {
    "package": "auditmation.generic.tls.collectorbot",
    "dataloader-version": "0.5.4",
    "import-artifact": "collectorbot"
  }
}
